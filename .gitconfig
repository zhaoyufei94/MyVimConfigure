[user]
	name = Yufei Zhao 
	email = yz3170@columbia.edu
[color]
	diff = auto
	status = auto
	branch = auto
[push]
	default = matching
[alias]
	co = checkout
	br = branch
	ci = commit -m
	st = status
	last = log -1 HEAD
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	unstage = checkout -
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
